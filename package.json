{
  "name": "@bruhabruh/theme",
  "version": "3.4.0",
  "type": "module",
  "description": "BruhaBruh Theme with CLI",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/theme"
  },
  "files": [
    "dist",
    "template"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli.mjs",
  "scripts": {
    "build": "tsup",
    "generate": "pnpm run build --silent && node ./dist/cli.mjs generate",
    "generate:schemes": "tsx ./src/generate-schemes.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm run lint:js && pnpm run lint:other",
    "lint:js": "eslint . --report-unused-disable-directives",
    "lint:other": "prettier --check '**/*.{md,yaml,yml,json}'",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.0.0",
    "@bruhabruh/prettier-config": "^1.1.0",
    "@bruhabruh/tsconfig": "^1.0.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@types/culori": "^2.1.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.9",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bruhabruh/type-safe": "^1.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "culori": "^4.0.1",
    "glob": "^11.0.0",
    "math-expression-evaluator": "^2.0.5",
    "ts-deepmerge": "^7.0.1",
    "yaml": "^2.4.5",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3"
  },
  "peerDependencies": {
    "tailwindcss": "^3"
  }
}
