{
  "name": "@bruhabruh/theme",
  "version": "4.0.3",
  "type": "module",
  "description": "BruhaBruh Theme with CLI",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/theme"
  },
  "files": [
    "dist",
    "template"
  ],
  "bin": "./dist/cli.mjs",
  "scripts": {
    "build": "tsup",
    "generate": "pnpm run build && node ./dist/cli.mjs generate",
    "generate:schemes": "tsx ./src/generate-schemes.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm run lint:js && pnpm run lint:other",
    "lint:js": "eslint . --report-unused-disable-directives",
    "lint:other": "prettier --check '**/*.{md,yaml,yml,json}'",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.6",
    "@bruhabruh/prettier-config": "^1.1.5",
    "@bruhabruh/tsconfig": "^1.0.4",
    "@bruhabruh/type-safe": "^1.2.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@types/culori": "^4.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "culori": "^4.0.2",
    "glob": "^11.0.3",
    "math-expression-evaluator": "^2.0.7",
    "ts-deepmerge": "^7.0.3",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  }
}
