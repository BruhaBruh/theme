{
  "name": "@bruhabruh/theme",
  "version": "4.0.0-beta.1",
  "type": "module",
  "description": "BruhaBruh Theme with CLI",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/theme"
  },
  "files": [
    "dist",
    "template"
  ],
  "bin": "./dist/cli.mjs",
  "scripts": {
    "build": "tsup",
    "generate": "pnpm run build && node ./dist/cli.mjs generate",
    "generate:schemes": "tsx ./src/generate-schemes.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "pnpm run lint:js && pnpm run lint:other",
    "lint:js": "eslint . --report-unused-disable-directives",
    "lint:other": "prettier --check '**/*.{md,yaml,yml,json}'",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.2",
    "@bruhabruh/prettier-config": "^1.1.4",
    "@bruhabruh/tsconfig": "^1.0.4",
    "@bruhabruh/type-safe": "^1.2.2",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@types/culori": "^2.1.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.14",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material/material-color-utilities": "^0.3.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "culori": "^4.0.1",
    "glob": "^11.0.1",
    "math-expression-evaluator": "^2.0.6",
    "ts-deepmerge": "^7.0.2",
    "yaml": "^2.7.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.4"
  }
}
